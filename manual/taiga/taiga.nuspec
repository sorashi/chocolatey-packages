<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
  <metadata>
    <id>taiga</id>
    <version>1.4.0</version>
    <owners>sorashi</owners>
    <packageSourceUrl>https://github.com/Sorashi/chocolatey-packages</packageSourceUrl>

    <title>Taiga</title>
    <authors>erengy</authors>
    <copyright>2022 Eren Okka</copyright>
    <projectUrl>https://taiga.moe/</projectUrl>
    <iconUrl>https://taiga.moe/assets/img/icon/taiga_256px.png</iconUrl>

    <licenseUrl>https://github.com/erengy/taiga/blob/master/LICENSE</licenseUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <projectSourceUrl>https://github.com/erengy/taiga/</projectSourceUrl>
    <docsUrl>https://github.com/erengy/taiga/wiki</docsUrl>
    <mailingListUrl>https://github.com/erengy/taiga/discussions</mailingListUrl>
    <bugTrackerUrl>https://github.com/erengy/taiga/issues</bugTrackerUrl>
    <tags>taiga anime</tags>
    <summary>A lightweight anime tracker for Windows</summary>
    <description>Taiga is an open-source desktop application for Windows. It automatically detects the anime videos you watch on your computer and synchronizes your progress with online services. It helps you manage your anime library, discover new series, share watched episodes and download new ones.</description>
    <releaseNotes>https://github.com/erengy/taiga/wiki/Changelog</releaseNotes>
    <dependencies>
      <dependency id="chocolatey-misc-helpers.extension" />
    </dependencies>
  </metadata>
  <files>
    <file src="tools\**" target="tools" />
  </files>
</package>
